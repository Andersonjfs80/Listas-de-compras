{
  "AppName": "app-backend-produto",
  "PathBase": "/app-backend-produto",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=sqlserver;Database=gestaoFinanceiraDB;User Id=sa;Password=Dev@123456;TrustServerCertificate=True;"
  },
  "AllowedHosts": "*",
  "Core_Http_Settings": {
    "DesativarSegurancaSsl": false
  },
  "ProdutoSettings": {},
  "KafkaSettings": {
    "BootstrapServers": "kafka:29092",
    "Topic": "topic-listas-compras",
    "Enabled": true,
    "LogAutomaticoHttp": true,
    "LogConsole": true,
    "OfuscarCampos": {
      "Request": [
        "senha",
        "password",
        "token"
      ],
      "Response": [
        "senha",
        "password",
        "token"
      ]
    }
  },
  "CacheSettings": {
    "ConnectionString": "redis:6379",
    "Enabled": true,
    "DefaultTTLMinutes": 5,
    "LogConsole": true
  }
}