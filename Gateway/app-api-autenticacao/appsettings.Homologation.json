{
    "AppName": "app-api-autenticacao",
    "PathBase": "/app-api-autenticacao",
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AutenticacaoSettings": {
        "UrlBackend": "http://app-backend-autenticacao:7001/app-backend-autenticacao"
    },
    "KafkaSettings": {
        "BootstrapServers": "kafka:29092"
    },
    "Core_Http_Settings": {
        "DesativarSegurancaSsl": true
    }
}